<% include partials/header.ejs%>
<% // include file specific css files here %>
<link type="text/css" rel="stylesheet" media="all" href="../css/search-result.css">
</head>
<body ng-app="tatilan" ng-controller="result" id="result" ng-style="style()" resize ng-cloak>


    <% include partials/nav.ejs %>
  <div class="main-parent">  
<main>
    <div id="map-search">
        <div class="search-list">
            <div class="filters">
                <div class="panel-body panel-light" style="padding-top: 30px; ">
                    <div class="row">
                      <div class="col-lg-2 col-md-12 text-center-sm text-center-md row-space-sm-1 filter-label pull-right" style="padding-right: 30px; padding-top: 5px;">
                        <label>{{copy.result.dateslabel}}</label>
                      </div>

                      <form class="col-lg-10 ">
                        <div class="row row-condensed">
                          <div class="col-md-4 col-sm-6 row-space-1-sm pull-right" style="padding-left: 0">
                            <label class="screen-reader-only" for="map-search-checkin">
                              {{copy.result.checkin}}
                            </label>
                            <input type="text" placeholder="{{copy.result.checkin}}" class="checkin tour-target ui-datepicker-target form-control" autocomplete="off" id="map-search-checkin" name="checkin">
                          </div>

                          <div class="col-md-4 col-sm-6 row-space-1-sm pull-right" style="padding-left: 0">
                            <label class="screen-reader-only" for="map-search-checkout">
                              {{copy.result.checkout}}
                            </label>
                            <input type="text" placeholder="{{copy.result.checkout}}" class="checkout tour-target ui-datepicker-target form-control" autocomplete="off" id="map-search-checkout" name="checkout">
                          </div>

                          <div class="col-md-4 col-sm-12 pull-right ">
                            <div class="dropdown">
                              <button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                                {{copy.result.guests2}}
                                <span class="caret"></span>
                              </button>
                              <ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
                                <li><a href="#">{{copy.result.guests1}}</a></li>
                                <li><a href="#">{{copy.result.guests2}}</a></li>
                                <li><a href="#">{{copy.result.guests3}}</a></li>
                                <li><a href="#">{{copy.result.guests4}}</a></li>
                                <li><a href="#">{{copy.result.guests5}}</a></li>
                                <li><a href="#">{{copy.result.guests6}}</a></li>
                                <li><a href="#">{{copy.result.guests7}}</a></li>
                                <li><a href="#">{{copy.result.guests8}}</a></li>
                                <li><a href="#">{{copy.result.guests9}}</a></li>
                                <li><a href="#">{{copy.result.guests10}}</a></li>
                                <li><a href="#">{{copy.result.guests11}}</a></li>
                                <li><a href="#">{{copy.result.guests12}}</a></li>

                              </ul>
</div>
                          </div>
                        </div>
                      </form>
                    </div>
                  </div>
                <div class="panel-body panel-light" style="padding-top: 20px; border-top: 1px solid #f7f7f7;">
                
                    <div class="row">
                      <div class="col-lg-2 col-md-12 text-center-sm text-center-md filter-label pull-right" style="padding-right: 30px; padding-top: 5px;">
                        <label>
                          {{copy.result.roomType}}
                          <i id="room-type-tooltip" class="icon icon-question hide-sm hide-md"></i>
                        </label>


                      </div>

                      <div class="col-lg-10">
                        <div id="room-options" class="row row-condensed">

                            <div class="col-middle-alt col-sm-4 pull-right">
                              <label class="checkbox facet-checkbox facet-checkbox--room-type panel panel-dark">
                                  <div class="facet-checkbox__icon-col">
                                    <i class="icon icon-entire-place h5 needsclick"></i>
                                  </div>
                                  <div class="facet-checkbox__label-col">
                                    <span>{{copy.result.entireHome}}</span>
                                  </div>
                                  <div class="facet-checkbox__input-col">
                                    <input type="checkbox" value="Entire home/apt" name="room-type">
                                  </div>
                              </label>
                            </div>
                            <div class="col-middle-alt col-sm-4 pull-right">
                              <label class="checkbox facet-checkbox facet-checkbox--room-type panel panel-dark">
                                  <div class="facet-checkbox__icon-col">
                                    <i class="icon icon-private-room h5 needsclick"></i>
                                  </div>
                                  <div class="facet-checkbox__label-col">
                                    <span>{{copy.result.privateRoom}}</span>
                                  </div>
                                  <div class="facet-checkbox__input-col">
                                    <input type="checkbox" value="Private room" name="room-type">
                                  </div>
                              </label>
                            </div>
                            

        </div>
      </div>
    </div>
  
                
                
                </div>
            </div> <!-- end of filters -->
            <div id="result-parent" ng-controller="searchResultController">
                <search-location-result id="345345"></search-location-result>
                <search-location-result id="345366"></search-location-result>
                <search-location-result id="345388"></search-location-result>
            </div>
            <div class="clearfix"></div>
            <div id="result-footer">
                <p>
          1 - 18 {{copy.result.paginationFrom}} 175 {{copy.result.paginationResult}}
        </p>
            <ul class="pagination">
                <li class="pull-right">
                    <a href="#" aria-label="Previous">
                    <span aria-hidden="true">&laquo;</span>
                  </a>
                  
                </li>
                <li class="pull-right"><a href="#">1</a></li>
                <li class="pull-right"><a href="#">2</a></li>
                <li class="pull-right"><a href="#">3</a></li>
                <li class="pull-right"><a href="#">4</a></li>
                <li class="pull-right"><a href="#">5</a></li>
                <li class="pull-right">
                    <a href="#" aria-label="Next">
                    <span aria-hidden="true">&raquo;</span>
                  </a>
                  
                </li>
              </ul>
            </div>
        </div>
        

        <div class="map" resulting-map ng-style="fullHeight(65)" style="width:40%">
        </div>

    </div>
    
</main>
    
    <script>
        window.user = <%- JSON.stringify(user) %>;
    </script>

    <% include partials/jsfiles.ejs%>
    <% // include file specific js files here %>
      
      <script type="text/javascript" src="../js/controllers/result.client.controller.js"></script>
     <script type="text/javascript" src="../js/controllers/signing.client.controller.js"></script>
   
</body>


</html>